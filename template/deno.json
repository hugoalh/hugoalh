{
	"$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
	"fmt": {
		"options": {
			"indentWidth": 4,
			"lineWidth": 800,
			"proseWrap": "preserve",
			"semiColons": true,
			"singleQuote": false,
			"useTabs": true
		}
	},
	"lint": {
		"rules": {
			"tags": [
				"recommended"
			]
		}
	}
}
